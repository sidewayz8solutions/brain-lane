# Brain Lane Environment Variables
# Copy this file to .env.local and fill in your values

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# ===========================================
# OPENAI / AI CONFIGURATION
# ===========================================
OPENAI_API_KEY=sk-your-openai-api-key
VITE_OPENAI_API_KEY=sk-your-openai-api-key

# Alternative AI Providers (optional)
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key

# ===========================================
# STRIPE BILLING
# ===========================================
STRIPE_SECRET_KEY=sk_live_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-publishable-key

# Stripe Price IDs
STRIPE_PRICE_PRO=price_your-pro-plan-id
STRIPE_PRICE_TEAM=price_your-team-plan-id
STRIPE_PRICE_ENTERPRISE=price_your-enterprise-plan-id

# ===========================================
# GPU PROVIDERS (Optional - for heavy inference)
# ===========================================
# RunPod
RUNPOD_API_KEY=your-runpod-api-key
VITE_RUNPOD_API_KEY=your-runpod-api-key

# Replicate
REPLICATE_API_TOKEN=your-replicate-api-token
VITE_REPLICATE_API_TOKEN=your-replicate-api-token

# LM Studio (Local)
LM_STUDIO_URL=http://localhost:1234/v1
VITE_LM_STUDIO_URL=http://localhost:1234/v1

# Ollama (Local)
OLLAMA_URL=http://localhost:11434
VITE_OLLAMA_URL=http://localhost:11434

# ===========================================
# GITHUB INTEGRATION (Optional)
# ===========================================
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret
GITHUB_PAT=ghp_your-personal-access-token

# ===========================================
# DEPLOYMENT PROVIDERS (Optional)
# ===========================================
# Vercel
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id

# Railway
RAILWAY_TOKEN=your-railway-token

# Fly.io
FLY_API_TOKEN=your-fly-api-token

# ===========================================
# APP CONFIGURATION
# ===========================================
VITE_APP_URL=http://localhost:5173
FRONTEND_URL=http://localhost:5173
NODE_ENV=development

# Feature Flags
VITE_ENABLE_GPU_WORKERS=false
VITE_ENABLE_MULTI_AGENT=true
VITE_ENABLE_BILLING=false
VITE_ENABLE_ANALYTICS=false

# ===========================================
# ANALYTICS & MONITORING (Optional)
# ===========================================
VITE_SENTRY_DSN=https://your-sentry-dsn
VITE_POSTHOG_KEY=your-posthog-api-key
VITE_POSTHOG_HOST=https://app.posthog.com
